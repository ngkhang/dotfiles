{
  // ---------- Security Settings ---------- //
  "security.workspace.trust.untrustedFiles": "open",
  // ---------- Window Settings ---------- //
  "window.titleBarStyle": "custom",
  "window.commandCenter": false,
  "window.autoDetectColorScheme": true,
  // ---------- Workbench Settings ---------- //
  "workbench.activityBar.location": "bottom",
  "workbench.sideBar.location": "right",
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.preferredDarkColorTheme": "Everforest Dark",
  "workbench.preferredLightColorTheme": "Everforest Light",
  "workbench.editor.showTabs": "multiple",
  "workbench.colorCustomizations": {
    "[Everforest Dark]": {
      "editorCursor.foreground": "#FFB703",
      "editorCursor.background": "#F2E8CF",
      "editor.selectionBackground": "#495057",
      "tab.activeBorder": "#FFC600"
    },
    "[Everforest Light]": {
      "editorCursor.foreground": "#6A994E",
      "editorCursor.background": "#F2E8CF",
      "editor.selectionBackground": "#E9EDC9",
      "tab.activeBorder": "#A7C957"
    }
  },
  "workbench.layoutControl.enabled": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorLargeFileConfirmation": 1024,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "none",
  "workbench.panel.showLabels": false,
  "workbench.tree.indent": 16,
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.enableStickyScroll": false,
  "workbench.settings.editor": "json",
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  // ---------- Editor Settings ---------- //
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontSize": 1,
  "editor.inlayHints.padding": true,
  "editor.inlayHints.fontFamily": "FiraCode Nerd Font",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 5,
  "editor.codeLensFontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "600",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 24,
  "editor.renderWhitespace": "all",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.semanticHighlighting.enabled": true,
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.scrollbar.horizontal": "hidden",
  "editor.guides.indentation": false,
  "editor.defaultFormatter": "prettier.prettier-vscode",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.lightbulb.enabled": "off",
  "editor.stickyScroll.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "always"
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,
  "editor.linkedEditing": true,
  "editor.fontFamily": "JetBrains Mono",
  "editor.largeFileOptimizations": false,
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "editor.folding": true,
  "editor.glyphMargin": true,
  "editor.suggestFontSize": 12,
  "editor.suggestLineHeight": 30,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.autoIndent": "full",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": true
  },
  // ---------- File Settings ---------- //
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.associations": {
    "*.sql": "sql",
    "*.tsx": "typescriptreact",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.js": "javascript"
  },
  "files.eol": "\n",
  // Toggle excluded files extension, refer for more detail https://github.com/jellydn/vscode-toggle-excluded-files
  "files.exclude": {
    "**/bin": true,
    "**/git": true,
    "**/.turbo": true,
    "**/.vercel": true,
    "**/.changes": true,
    "**/.yarn": true,
    "**/.vscode": true,
    "**/.vscode-test": true,
    "**/.git": true,
    "**/.github": true,
    "**/.grit": true,
    "**/.changeset": true,
    "**/node_modules": true,
    "**/.husky": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/cdk.out": true,
    "**/coverage": true,
    "**/.next": true,
    "**/*.tsbuildinfo": true,
    "**/*.nyc_output": true,
    "**/*.tap": true,
    "**/*.econ": true,
    "**/.astro": true,
    "**/.encore": true,
    "**/encore.gen": true
  },
  "files.trimTrailingWhitespace": true,
  // ---------- Terminal Settings ---------- //
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": false,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell 7",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "PowerShell 7": {
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.enableMultiLinePasteWarning": "always",
  "terminal.integrated.minimumContrastRatio": 7,
